    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "mysql": "^2.15.0",
    "ngrx-store-freeze": "^0.2.4",
    "pg": "^7.4.3",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.1",
